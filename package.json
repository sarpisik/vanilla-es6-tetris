  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "server": "http-server dist",
    "demo": "npm run build && npm run server"
  },
  "keywords": [
    "vanilla, boilerplate"
  ],
